{
  "version": "1.2.0-*",
  "authors": [ "LeoLaw" ],
  "dependencies": {
    "Newtonsoft.Json": "9.0.1"
  },
  "frameworks": {
    "netstandard11": {
      "buildOptions": { "define": [ "NETSTANDARD", "NETCORE", "DOTNET" ] },
      "dependencies": {
        "System.Net.Http": "4.3.0"
      }
    }
  },
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG", "TRACE" ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "RELEASE", "TRACE" ],
        "optimize": true
      }
    }
  },
  "packOptions": {
    "summary": "A restful http client",
    "tags": [ "rest", "restful", "http" ],
    "owners": [ "LeoLaw" ],
    "requireLicenseAcceptance": false
  }
}
