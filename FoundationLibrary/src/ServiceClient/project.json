{
  "version": "1.1.0-*",
  "authors": [ "LeoLaw" ],
  "frameworks": {
    "net4": {
      "dependencies": {
        "Microsoft.Bcl.Async": "1.0.168",
        "Microsoft.Net.Http": "2.2.29",
        "Newtonsoft.Json": "6.0.8"
      },
      "buildOptions": { "define": [ "NET4", "DOTNET" ] }
    },
    "net45": {
      "buildOptions": { "define": [ "NET45", "DOTNET" ] },
      "dependencies": {
        "Newtonsoft.Json": "6.0.8"
      },
      "frameworkAssemblies": {
        "System.Net.Http": "4.0.0"
      }
    },
    "netstandard13": {
      "buildOptions": { "define": [ "NETSTANDARD", "NETCORE" ] },
      "dependencies": {
        "Newtonsoft.Json": "9.0.1",
        "System.Net.Http": "4.1.0"
      }
    }
  },
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG", "TRACE" ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "RELEASE", "TRACE" ],
        "optimize": true
      }
    }
  },
  "packOptions": {
    "summary": "A restful http client",
    "tags": [ "rest", "restful", "http" ],
    "owners": [ "LeoLaw" ],
    "requireLicenseAcceptance": false
  }
}
