{
  "version": "1.3.1-*",
  "authors": [ "LeoLaw" ],
  "dependencies": {
    "Newtonsoft.Json": "9.0.1"
  },
  "frameworks": {
    "netstandard13": {
      "buildOptions": {
        "define": [ "NETSTANDARD", "NETCORE", "DOTNET" ]
      },
      "dependencies": {
        "System.Net.Http": "4.3.0"
      }
    },
    "net46": {
      "buildOptions": {
        "define": [ "NET46", "DOTNET" ]
      },
      "dependencies": {
        "System.Net.Http": "4.0.0"
      }
    },
    "net45": {
      "buildOptions": {
        "define": [ "NET45", "DOTNET" ]
      },
      "dependencies": {
        "System.Net.Http": "4.0.0"
      }
    },
    "net40": {
      "buildOptions": {
        "define": [ "NET4", "DOTNET" ]
      },
      "dependencies": {
        "Microsoft.Net.Http": "2.2.29",
        "Microsoft.Bcl.Async": "1.0.168"
      }
    }
  },
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG", "TRACE" ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "RELEASE", "TRACE" ],
        "optimize": true,
        "xmlDoc": true
      }
    }
  },
  "packOptions": {
    "summary": "A restful http client",
    "tags": [ "rest", "restful", "http" ],
    "owners": [ "LeoLaw" ],
    "requireLicenseAcceptance": false
  }
}
